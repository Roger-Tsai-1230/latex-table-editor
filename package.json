{
  "name": "latex-table-editor",
  "displayName": "latex-table-editor",
  "description": "A modern LaTeX table editor for VS Code, supporting WYSIWYG editing, color, merging, and LaTeX code generation.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/latex-table-editor.git"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:latex-table-editor.openEditor",
    "onCommand:latex-table-editor.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "latex-table-editor.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "latex-table-editor.openEditor",
        "title": "Open LaTeX Table Editor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
